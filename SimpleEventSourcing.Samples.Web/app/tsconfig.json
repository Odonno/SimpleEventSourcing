        "noEmitOnError": true,
        "removeComments": true,
        "sourceMap": true,
        "moduleResolution": "node",
        "target": "es6",
        "declaration": false
    },
    "compileOnSave": true,
    "include": [
        "**/**.ts"
    ],
    "exclude": [
        "node_modules"
    ]
